    angular
        .module('app.default-translations', ['pascalprecht.translate'])
        .config(loadTranslations)
        .run(reportLoading);

    loadTranslations.$inject = ['$translateProvider'];

    function loadTranslations($translateProvider) {
        var localeRegex = /^[a-z]{2}_[A-Z]{2}$/;

        if (angular.isObject(translationData) && localeRegex.test(locale)) {
            $translateProvider.translations(locale, translationData);
            translationsLoaded = true;
        }
    }

    reportLoading.$inject = ['$log'];

    function reportLoading($log) {
        if (translationsLoaded) {
            $log.info('[CDS] Pre-loaded translations for', locale);
        } else {
            $log.warn('[CDS] Pre-loaded translations not loaded', locale, translationData);
        }
    }
})(angular);
